{
  "name": "binaryen-webpack-plugin",
  "version": "0.1.0",
  "description": "Prepare compressed versions of assets with Binaryen",
  "author": "Alon Zakai @kripken",
  "main": "dist/cjs.js",
  "files": [
    "dist"
  ],
  "license": "Apache 2",
  "scripts": {
    "start": "npm run build -- -w",
    "build": "cross-env NODE_ENV=production babel src -d dist --ignore 'src/**/*.test.js'",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "release": "standard-version",
    "test": "jest",
    "travis:test": "npm run test -- --runInBand",
    "webpack-defaults": "webpack-defaults"
  },
  "dependencies": {
    "binaryen": "^46.0.0",
  },
  "devDependencies": {
    "jest": "^21.2.1",
    "webpack": "^3.8.1",
    "webpack-defaults": "^1.6.0"
  },
  "engines": {
    "node": ">= 5.10"
  },
  "peerDependencies": {
    "webpack": "^2.0.0 || ^3.0.0"
  },
  "homepage": "https://github.com/kripken/binaryen-webpack-plugin",
  "repository": {
    "type": "git",
    "url": "git+https://https://github.com/kripken/binaryen-webpack-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/kripken/binaryen-webpack-plugin/issues"
  },
}
